<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >


<mapper namespace="com.gitnavi.springboot.springbootsimpledemo.mapper.SysUserMapper">

    <sql id="Base_Column_List">
        id, login_name, email, delete_enum, create_date, lock_version
    </sql>

    <!--=================================================================================-->

    <select id="findAll" resultType="com.gitnavi.springboot.springbootsimpledemo.pojo.entity.SysUser">
        SELECT
        <include refid="Base_Column_List"/>
        FROM sys_user
    </select>


</mapper>
